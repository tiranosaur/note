//      Simple Http
    use http_req::request;

    fn main() {
        let mut writer = Vec::new();
        let res = request::get("http://test.local/", &mut writer).unwrap();
        let result = std::str::from_utf8(&writer).unwrap();

        println!("Status: {} {}", res.status_code(), res.reason());
        println!("{}", result);
    }
