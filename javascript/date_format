var lastUpdate = new Date('2020-02-25 23:13:18');
var month = new Intl.DateTimeFormat("en-US", {month: "long"}).format(lastUpdate);
var time = new Intl.DateTimeFormat("en-US", {hour: '2-digit', minute: '2-digit', hour12: true}).format(lastUpdate);
lastUpdate.getDate() + ' ' + month + ' ' + lastUpdate.getFullYear() + ' At ' + time.toLowerCase();
