brew install colima
colima start --arch x86_64 --memory 4

docker run --name oracle -p 1522:1521 -e ORACLE_PASSWORD=Pass123 gvenzl/oracle-xe
docker run --name redis -p 7001:6379 -d redis
docker run --name elasticsearch -p 9201:9200 -e "discovery.type=single-node" -d docker.elastic.co/elasticsearch/elasticsearch:8.5.0

default oracle SID: XE


colima stop && colima start --arch x86_64 --memory 4 &&  docker start $(docker ps -a -q) && docker ps
