package com.company;


import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;
import java.util.Random;
import java.util.stream.IntStream;

public class Main {
    public static void main(String[] args) {
        StringBuilder result = new StringBuilder();
        String[] ext = {" ", "-", "_", "\r\n"};
        List<String> list = new ArrayList<>(Arrays.asList(ext));
        IntStream.concat(IntStream.range('a', 'z'), IntStream.range('A', 'Z')).forEach(x -> list.add(Character.toString((char) x)));
        IntStream.range(1, 100).forEach(x -> result.append(list.get(getRandom(0, list.size()))));
        System.out.println(result);
    }

    public static int getRandom(int start, int end) {
        return (int) ((new Random()).nextDouble() * (end) - start);
    }
}
