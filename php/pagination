public function paginationRange($table, $numPage, $paginagion, $range = 5)
    {
        $count = DB::query("SELECT id FROM $table");
        $countPage = floor(count($count) / $paginagion);
        $halfRange = ($range - 1) / 2;
        if ($countPage < $range) return [1, $countPage];
        else if ($numPage - $halfRange < 1) return [1, $range];
        else if ($numPage + $halfRange > $countPage) return [$countPage - $range+1, $countPage];
        else return [$numPage-$halfRange,$numPage+$halfRange];
    }
